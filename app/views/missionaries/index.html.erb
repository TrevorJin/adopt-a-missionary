<p id="notice"><%= notice %></p>

<h1>Missionaries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Campus</th>
      <th>Image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @missionaries.each do |missionary| %>
      <tr>
        <td><%= missionary.name %></td>
        <td><%= missionary.campus %></td>
        <td><%= missionary.image_url %></td>
        <td><%= link_to 'Show', missionary %></td>
        <td><%= link_to 'Edit', edit_missionary_path(missionary) %></td>
        <td><%= link_to 'Destroy', missionary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Missionary', new_missionary_path %>
